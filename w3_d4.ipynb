        "from transformers import AutoTokenizer\n",
        "import pandas as pd\n",
        "from IPython.display import display, HTML\n",
        "\n",
        "def explore_tokenization(text, tokenizer_names):\n",
        "    \"\"\"\n",
        "    Compare tokenization across different Arabic models\n",
        "    \"\"\"\n",
        "    print(f\"\\nOriginal text: {text}\")\n",
        "\n",
        "    results = []\n",
        "    tokenizers = {}\n",
        "\n",
        "    # Initialize tokenizers\n",
        "    for name in tokenizer_names:\n",
        "        tokenizers[name] = AutoTokenizer.from_pretrained(name)\n",
        "\n",
        "    # Process with each tokenizer\n",
        "    for name, tokenizer in tokenizers.items():\n",
        "        tokens = tokenizer.tokenize(text)\n",
        "        token_ids = tokenizer.encode(text)\n",
        "\n",
        "        results.append({\n",
        "            \"Model\": name.split(\"/\")[-1],  # Get just the model name part\n",
        "            \"Token Count\": len(tokens),\n",
        "            \"Tokens\": tokens,\n",
        "            \"Token IDs\": token_ids\n",
        "        })\n",
        "\n",
        "    # Create a DataFrame for comparison\n",
        "    df = pd.DataFrame(results)\n",
        "\n",
        "    # Display token counts\n",
        "    print(\"\\nToken counts comparison:\")\n",
        "    display(df[[\"Model\", \"Token Count\"]])\n",
        "\n",
        "    # Display detailed tokens (expand the DataFrame for all models)\n",
        "    print(\"\\nDetailed tokenization comparison:\")\n",
        "    for i, row in df.iterrows():\n",
        "        print(f\"\\n{row['Model']}:\")\n",
        "        print(f\"Token count: {row['Token Count']}\")\n",
        "        print(f\"Tokens: {row['Tokens']}\")\n",
        "\n",
        "    return df\n",
        "\n",
        "# Test with different Arabic text samples\n",
        "sample_texts = [\n",
        "    \"أهلاً وسهلاً بكم في دورة معالجة اللغة العربية الطبيعية\",  # MSA\n",
        "    \"شلونك؟ شخبارك اليوم؟\",  # Gulf dialect\n",
        "    \"إزيك؟ عامل إيه؟\",  # Egyptian dialect\n",
        "]\n",
        "\n",
        "tokenizer_names = [\n",
        "    \"aubmindlab/bert-base-arabertv2\",\n",
        "    \"UBC-NLP/MARBERT\",\n",
        "    \"CAMeL-Lab/bert-base-arabic-camelbert-mix\"\n",
        "]\n",
        "\n",
        "# Compare tokenization across models and dialects\n",
        "print(\"TOKENIZATION COMPARISON ACROSS ARABIC MODELS\")\n",
        "print(\"=\" * 50)\n",
        "\n",
        "dfs = []\n",
        "for i, text in enumerate(sample_texts):\n",
        "    print(f\"\\nText {i+1} Analysis:\")\n",
        "    print(\"-\" * 30)\n",
        "    if i == 0:\n",
        "        dialect = \"Modern Standard Arabic (MSA)\"\n",
        "    elif i == 1:\n",
        "        dialect = \"Gulf Dialect\"\n",
        "    else:\n",
        "        dialect = \"Egyptian Dialect\"\n",
        "    print(f\"Dialect: {dialect}\")\n",
        "\n",
        "    df = explore_tokenization(text, tokenizer_names)\n",
        "    dfs.append(df)\n",
        "    print(\"-\" * 50)\n",
        "\n",
        "# Visual comparison of token counts\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "# Extract token counts for plotting\n",
        "models = [name.split(\"/\")[-1] for name in tokenizer_names]\n",
        "token_counts = np.zeros((len(sample_texts), len(models)))\n",
        "\n",
        "for i, df in enumerate(dfs):\n",
        "    for j, model in enumerate(models):\n",
        "        token_counts[i, j] = df[df[\"Model\"] == model][\"Token Count\"].values[0]\n",
        "\n",
        "# Plot\n",
        "fig, ax = plt.subplots(figsize=(10, 6))\n",
        "x = np.arange(len(sample_texts))\n",
        "width = 0.25\n",
        "\n",
        "for i, model in enumerate(models):\n",
        "    ax.bar(x + i*width - width, token_counts[:, i], width, label=model)\n",
        "\n",
        "ax.set_ylabel('Token Count')\n",
        "ax.set_title('Number of Tokens by Model and Dialect')\n",
        "ax.set_xticks(x)\n",
        "ax.set_xticklabels(['MSA', 'Gulf', 'Egyptian'])\n",
        "ax.legend()\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ]
