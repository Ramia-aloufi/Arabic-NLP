      "source": [
        "from transformers import pipeline, AutoTokenizer, AutoModelForTokenClassification\n",
        "\n",
        "# Load a pre-trained Arabic NER model\n",
        "model_name = \"CAMeL-Lab/bert-base-arabic-camelbert-mix-ner\"\n",
        "tokenizer = AutoTokenizer.from_pretrained(model_name)\n",
        "model = AutoModelForTokenClassification.from_pretrained(model_name)\n",
        "\n",
        "# Create NER pipeline\n",
        "ner_pipeline = pipeline(\"ner\", model=model, tokenizer=tokenizer, aggregation_strategy=\"simple\")\n",
        "\n",
        "# Sample Arabic texts\n",
        "texts = [\n",
        "    \"يعيش محمد في الرياض ويعمل في شركة جوجل.\",\n",
        "    \"زار الرئيس المصري عبد الفتاح السيسي المملكة العربية السعودية الأسبوع الماضي.\",\n",
        "    \"تأسست جامعة القاهرة في عام 1908 وتعتبر من أقدم الجامعات في الشرق الأوسط.\"\n",
        "]\n",
        "\n",
        "# Process texts\n",
        "for i, text in enumerate(texts):\n",
        "    print(f\"\\nText {i+1}: {text}\")\n",
        "    entities = ner_pipeline(text)\n",
        "\n",
        "    print(\"Recognized entities:\")\n",
        "    for entity in entities:\n",
        "        print(f\"  • {entity['word']} - {entity['entity_group']} (Score: {entity['score']:.4f})\")\n",
        "\n",
        "    # Visualize the entities in text (simple version)\n",
        "    words = text.split()\n",
        "    entity_spans = [(entity['start'], entity['end'], entity['entity_group']) for entity in entities]\n",
        "\n",
        "    # Sort spans by start position\n",
        "    entity_spans.sort(key=lambda x: x[0])\n",
        "\n",
        "    marked_text = text\n",
        "    offset = 0\n",
        "    for start, end, label in entity_spans:\n",
        "        insert_before = f\"[{label}-START]\"\n",
        "        insert_after = f\"[{label}-END]\"\n",
        "\n",
        "        marked_text = marked_text[:start+offset] + insert_before + marked_text[start+offset:end+offset] + insert_after + marked_text[end+offset:]\n",
        "        offset += len(insert_before) + len(insert_after)\n",
        "\n",
        "    print(\"\\nMarked text:\")\n",
        "    print(marked_text)"
      ]
    },
