        "from transformers import AutoTokenizer, AutoModel\n",
        "import re\n",
        "import pyarabic.araby as araby\n",
        "import matplotlib.pyplot as plt\n",
        "import time\n",
        "\n",
        "class ArabicPreprocessor:\n",
        "    def __init__(self, model_name=\"UBC-NLP/MARBERT\"):\n",
        "        start = time.time()\n",
        "        self.tokenizer = AutoTokenizer.from_pretrained(model_name)\n",
        "        self.model_name = model_name\n",
        "        self.load_time = time.time() - start\n",
        "        print(f\"Initialized preprocessor with {model_name}\")\n",
        "        print(f\"Tokenizer loaded in {self.load_time:.2f} seconds\")\n",
        "\n",
        "    def clean_text(self, text, normalize_hamza=True, remove_diacritics=True,\n",
        "                  remove_non_arabic=False, verbose=False):\n",
        "        \"\"\"\n",
        "        Comprehensive cleaning of Arabic text with configurable options\n",
        "        \"\"\"\n",
        "        steps = []\n",
        "        current = text\n",
        "        steps.append((\"Original\", current))\n",
        "\n",
        "        # Remove diacritics (tashkeel) if requested\n",
        "        if remove_diacritics:\n",
        "            current = araby.strip_tashkeel(current)\n",
        "            current = araby.strip_tatweel(current)\n",
        "            steps.append((\"Remove diacritics & tatweel\", current))\n",
        "\n",
        "        # Normalize hamza and other characters if requested\n",
        "        if normalize_hamza:\n",
        "            current = araby.normalize_hamza(current)\n",
        "            current = araby.normalize_alef(current)\n",
        "            steps.append((\"Normalize hamza & lam-alef\", current))\n",
        "\n",
        "        # Remove non-Arabic characters if requested\n",
        "        if remove_non_arabic:\n",
        "            current = re.sub(r'[^\\u0600-\\u06FF\\s]', ' ', current)\n",
        "            steps.append((\"Remove non-Arabic\", current))\n",
        "\n",
        "        # Always remove extra spaces\n",
        "        current = re.sub(r'\\s+', ' ', current).strip()\n",
        "        steps.append((\"Clean whitespace\", current))\n",
        "\n",
        "        # Show step-by-step process if verbose\n",
        "        if verbose:\n",
        "            for step_name, step_text in steps:\n",
        "                print(f\"{step_name}: {step_text}\")\n",
        "\n",
        "        return current\n",
        "\n",
        "    def preprocess(self, text, return_tensors=None, max_length=128,\n",
        "                   clean_options=None, visualize=False):\n",
        "        \"\"\"\n",
        "        Full preprocessing pipeline with visualization options\n",
        "        \"\"\"\n",
        "        # Default cleaning options\n",
        "        if clean_options is None:\n",
        "            clean_options = {\n",
        "                \"normalize_hamza\": True,\n",
        "                \"remove_diacritics\": True,\n",
        "                \"remove_non_arabic\": False,\n",
        "                \"verbose\": visualize\n",
        "            }\n",
        "\n",
        "        # Step 1: Clean the text\n",
        "        cleaned_text = self.clean_text(text, **clean_options)\n",
        "\n",
        "        # Step 2: Tokenize with model's tokenizer\n",
        "        if return_tensors:\n",
        "            # For model input (ready for inference)\n",
        "            tokenized = self.tokenizer(\n",
        "                cleaned_text,\n",
        "                return_tensors=return_tensors,\n",
        "                padding=True,\n",
        "                truncation=True,\n",
        "                max_length=max_length\n",
        "            )\n",
        "            result = tokenized\n",
        "\n",
        "            # Add original and cleaned text for reference\n",
        "            result.original_text = text\n",
        "            result.cleaned_text = cleaned_text\n",
        "\n",
        "        else:\n",
        "            # For inspection/analysis\n",
        "            tokens = self.tokenizer.tokenize(cleaned_text)\n",
        "            token_ids = self.tokenizer.encode(cleaned_text)\n",
        "\n",
        "            result = {\n",
        "                \"original\": text,\n",
        "                \"cleaned\": cleaned_text,\n",
        "                \"tokens\": tokens,\n",
        "                \"token_ids\": token_ids,\n",
        "                \"token_count\": len(tokens)\n",
        "            }\n",
        "\n",
        "        # Visualize the tokenization if requested\n",
        "        if visualize:\n",
        "            self._visualize_tokenization(result)\n",
        "\n",
        "        return result\n",
        "\n",
        "    def _visualize_tokenization(self, result):\n",
        "        \"\"\"Visualize the tokenization process\"\"\"\n",
        "        # Get tokens and original text\n",
        "        if isinstance(result, dict):\n",
        "            tokens = result[\"tokens\"]\n",
        "            original = result[\"original\"]\n",
        "            cleaned = result[\"cleaned\"]\n",
        "        else:\n",
        "            # Handle tensor output\n",
        "            tokens = self.tokenizer.convert_ids_to_tokens(result.input_ids[0])\n",
        "            original = result.original_text\n",
        "            cleaned = result.cleaned_text\n",
        "\n",
        "        # Create visualization\n",
        "        fig, ax = plt.subplots(figsize=(12, 4))\n",
        "        fig.patch.set_facecolor('white')\n",
        "\n",
        "        # Title and setup\n",
        "        plt.title(f\"Tokenization with {self.model_name}\", fontsize=14)\n",
        "        ax.axis('off')\n",
        "\n",
        "        # Display original and cleaned text\n",
        "        text_y = 0.9\n",
        "        ax.text(0.01, text_y, \"Original:\", fontweight='bold', fontsize=12)\n",
        "        ax.text(0.15, text_y, original, fontsize=12)\n",
        "\n",
        "        ax.text(0.01, text_y-0.15, \"Cleaned:\", fontweight='bold', fontsize=12)\n",
        "        ax.text(0.15, text_y-0.15, cleaned, fontsize=12)\n",
        "\n",
        "        # Display tokens\n",
        "        ax.text(0.01, text_y-0.3, \"Tokens:\", fontweight='bold', fontsize=12)\n",
        "\n",
        "        # Calculate token positions (for visualization)\n",
        "        token_positions = []\n",
        "        curr_x = 0.15\n",
        "        for i, token in enumerate(tokens):\n",
        "            # Display token with index\n",
        "            y_pos = text_y-0.3-0.05*(i//8)  # Put 8 tokens per line\n",
        "            x_pos = curr_x if i % 8 == 0 else token_positions[-1][0] + len(token_positions[-1][1])*0.015 + 0.02\n",
        "\n",
        "            # Save position for next token\n",
        "            token_positions.append((x_pos, token))\n",
        "\n",
        "            # Draw token box\n",
        "            token_display = token.replace('##', '')\n",
        "            ax.text(x_pos, y_pos, token_display,\n",
        "                    bbox=dict(facecolor='lightblue', alpha=0.3, pad=5),\n",
        "                    fontsize=10)\n",
        "\n",
        "            # Add token index\n",
        "            ax.text(x_pos, y_pos-0.03, f\"{i}\", fontsize=8, color='red')\n",
        "\n",
        "        # Add total token count\n",
        "        ax.text(0.01, 0.1, f\"Total tokens: {len(tokens)}\", fontweight='bold', fontsize=12)\n",
        "\n",
        "        plt.tight_layout()\n",
        "        plt.show()\n",
        "\n",
        "# Usage examples\n",
        "print(\"ARABIC PREPROCESSING PIPELINE EXAMPLES\")\n",
        "print(\"=\" * 50)\n",
        "\n",
        "# Create the preprocessor\n",
        "preprocessor = ArabicPreprocessor()\n",
        "\n",
        "# Example 1: Basic preprocessing for inspection\n",
        "print(\"\\nExample 1: Basic text analysis\")\n",
        "print(\"-\" * 30)\n",
        "\n",
        "text1 = \"السَّلَامُ عَلَيْكُمْ وَرَحْمَةُ اللهِ - مرحباً بكم في المدينة المنورة عام 2025!\"\n",
        "result1 = preprocessor.preprocess(text1, visualize=True)\n",
        "\n",
        "print(f\"Original: {result1['original']}\")\n",
        "print(f\"Cleaned:  {result1['cleaned']}\")\n",
        "print(f\"Token count: {result1['token_count']}\")\n",
        "print(f\"First 5 tokens: {result1['tokens'][:5]}...\")\n",
        "print(\"-\" * 50)\n",
        "\n",
        "# Example 2: Preparing for model input\n",
        "print(\"\\nExample 2: Preparing for model inference\")\n",
        "print(\"-\" * 30)\n",
        "\n",
        "text2 = \"أنا أحبّ تعلم اللغة العربية واستخدام الذكاء الاصطناعي!\"\n",
        "model_input = preprocessor.preprocess(text2, return_tensors=\"pt\", visualize=True)\n",
        "\n",
        "print(\"Model input format:\")\n",
        "for key, value in model_input.items():\n",
        "    if key not in ['original_text', 'cleaned_text']:\n",
        "        print(f\"{key}: {value}\")\n",
        "print(\"-\" * 50)\n",
        "\n",
        "# Example 3: Comparing dialectal text vs MSA\n",
        "print(\"\\nExample 3: Dialectal text analysis\")\n",
        "print(\"-\" * 30)\n",
        "\n",
        "texts = [\n",
        "    \"كيف حالك؟ أنا بخير الحمد لله.\",  # MSA\n",
        "    \"شلونك؟ أنا زين الحمد لله.\",      # Gulf\n",
        "    \"ازيك؟ أنا كويس الحمد لله.\"        # Egyptian\n",
        "]\n",
        "\n",
        "# Process each text\n",
        "for i, text in enumerate(texts):\n",
        "    dialect = [\"MSA\", \"Gulf\", \"Egyptian\"][i]\n",
        "    print(f\"\\nDialect: {dialect}\")\n",
        "    result = preprocessor.preprocess(text, visualize=False)\n",
        "    print(f\"Original: {result['original']}\")\n",
        "    print(f\"Tokens: {result['tokens']}\")\n",
        "    print(f\"Token count: {result['token_count']}\")"
      ]
