        "from transformers import AutoModelForSequenceClassification, AutoTokenizer\n",
        "from transformers import TrainingArguments, Trainer\n",
        "import numpy as np\n",
        "import evaluate\n",
        "from datasets import load_dataset\n",
        "\n",
        "# 1. Load pre-trained model and tokenizer\n",
        "model_name = \"aubmindlab/bert-base-arabertv2\"\n",
        "tokenizer = AutoTokenizer.from_pretrained(model_name)\n",
        "\n",
        "# 2. Load the Arabic sentiment Twitter dataset\n",
        "arabic_sentiment_dataset = load_dataset(\"asas-ai/Arabic_Sentiment_Twitter_Corpus\")\n",
        "print(f\"Dataset loaded: {arabic_sentiment_dataset}\")\n",
        "\n",
        "# Create a subsample of the dataset for faster fine-tuning\n",
        "# Let's use 10% of the data for training and 10% for validation/test\n",
        "train_sample_size = 0.1\n",
        "eval_sample_size = 0.1\n",
        "\n",
        "# Subsample the dataset\n",
        "subsampled_dataset = arabic_sentiment_dataset.shuffle(seed=42)\n",
        "subsampled_dataset = {\n",
        "    \"train\": subsampled_dataset[\"train\"].select(range(int(len(subsampled_dataset[\"train\"]) * train_sample_size))),\n",
        "    \"test\": subsampled_dataset[\"test\"].select(range(int(len(subsampled_dataset[\"test\"]) * eval_sample_size)))\n",
        "}\n",
        "arabic_sentiment_dataset = subsampled_dataset\n",
        "\n",
        "# First, let's examine the structure of the dataset\n",
        "sample_example = subsampled_dataset[\"train\"][0]\n",
        "print(f\"Sample example: {sample_example}\")\n",
        "\n",
        "# Create label mapping dictionaries\n",
        "unique_labels = set()\n",
        "for split in subsampled_dataset.keys():\n",
        "    for example in subsampled_dataset[split]:\n",
        "        unique_labels.add(example[\"label\"])\n",
        "\n",
        "labels = sorted(list(unique_labels))\n",
        "id2label = {i: label for i, label in enumerate(labels)}\n",
        "label2id = {label: i for i, label in enumerate(labels)}\n",
        "num_labels = len(labels)\n",
        "\n",
        "print(f\"Labels: {labels}\")\n",
        "print(f\"id2label: {id2label}\")\n",
        "print(f\"num_labels: {num_labels}\")\n",
        "\n",
        "# 3. Preprocess the dataset\n",
        "def preprocess_function(examples):\n",
        "    # Tokenize the texts\n",
        "    result = tokenizer(\n",
        "        examples[\"tweet\"],\n",
        "        truncation=True,\n",
        "        padding=\"max_length\",\n",
        "        max_length=128,\n",
        "        return_tensors=None  # Important: don't return tensors here\n",
        "    )\n",
        "\n",
        "    # Convert string labels to integers\n",
        "    result[\"labels\"] = [label2id[label] for label in examples[\"label\"]]\n",
        "\n",
        "    return result\n",
        "\n",
        "# Apply preprocessing\n",
        "tokenized_datasets = {}\n",
        "for split in subsampled_dataset.keys():\n",
        "    tokenized_datasets[split] = subsampled_dataset[split].map(\n",
        "        preprocess_function,\n",
        "        batched=True,\n",
        "        remove_columns=subsampled_dataset[split].column_names\n",
        "    )\n",
        "\n",
        "print(f\"Processed dataset structure: {tokenized_datasets}\")\n",
        "\n",
        "# 4. Define metrics for evaluation\n",
        "accuracy_metric = evaluate.load(\"accuracy\")\n",
        "f1_metric = evaluate.load(\"f1\")\n",
        "\n",
        "def compute_metrics(eval_pred):\n",
        "    predictions, labels = eval_pred\n",
        "    predictions = np.argmax(predictions, axis=1)\n",
        "    accuracy = accuracy_metric.compute(predictions=predictions, references=labels)\n",
        "    f1 = f1_metric.compute(predictions=predictions, references=labels, average=\"weighted\")\n",
        "    return {\n",
        "        \"accuracy\": accuracy[\"accuracy\"],\n",
        "        \"f1\": f1[\"f1\"]\n",
        "    }\n",
        "\n",
        "# 5. Initialize model with correct number of labels\n",
        "model = AutoModelForSequenceClassification.from_pretrained(\n",
        "    model_name,\n",
        "    num_labels=num_labels,\n",
        "    id2label=id2label,\n",
        "    label2id=label2id\n",
        ")\n",
        "\n",
        "# 6. Define training arguments with fewer epochs for faster training\n",
        "batch_size = 16\n",
        "training_args = TrainingArguments(\n",
        "    output_dir=\"./results_arabic_sentiment\",\n",
        "    learning_rate=5e-5,\n",
        "    per_device_train_batch_size=batch_size,\n",
        "    per_device_eval_batch_size=batch_size,\n",
        "    num_train_epochs=2,  # Reduced from 3 to 2 for faster training\n",
        "    weight_decay=0.01,\n",
        "    eval_strategy=\"epoch\",\n",
        "    save_strategy=\"epoch\",\n",
        "    load_best_model_at_end=True,\n",
        "    metric_for_best_model=\"f1\",\n",
        "    push_to_hub=False,\n",
        "    logging_steps=10,  # More frequent logging\n",
        "    warmup_steps=50,   # Fewer warmup steps\n",
        ")\n",
        "\n",
        "# 7. Initialize trainer\n",
        "trainer = Trainer(\n",
        "    model=model,\n",
        "    args=training_args,\n",
        "    train_dataset=tokenized_datasets[\"train\"],\n",
        "    eval_dataset=tokenized_datasets[\"test\"],\n",
        "    tokenizer=tokenizer,\n",
        "    compute_metrics=compute_metrics\n",
        ")\n",
        "\n",
        "# 8. Train the model\n",
        "print(\"Starting model training...\")\n",
        "trainer.train()\n",
        "\n",
        "# 9. Evaluate on test set\n",
        "eval_results = trainer.evaluate(tokenized_datasets[\"test\"])\n",
        "print(f\"Test results: {eval_results}\")\n",
        "\n",
        "# 10. Save the fine-tuned model\n",
        "model_path = \"./arabic_sentiment_model\"\n",
        "trainer.save_model(model_path)\n",
        "tokenizer.save_pretrained(model_path)\n",
        "print(f\"Model saved to {model_path}\")\n",
        "\n",
        "# 11. Test with new examples\n",
        "def predict_sentiment(text):\n",
        "    inputs = tokenizer(text, return_tensors=\"pt\", truncation=True, padding=True)\n",
        "    outputs = model(**inputs)\n",
        "    probabilities = outputs.logits.softmax(dim=1)\n",
        "    prediction = probabilities.argmax(dim=1).item()\n",
        "    confidence = probabilities[0][prediction].item()\n",
        "    sentiment = id2label[prediction]\n",
        "    return {\n",
        "        \"text\": text,\n",
        "        \"sentiment\": sentiment,\n",
        "        \"confidence\": confidence\n",
        "    }\n",
        "\n",
        "# Test with few examples\n",
        "test_texts = [\n",
        "    \"هذا الهاتف رائع، أحب كل شيء فيه!\",\n",
        "    \"المطعم سيء جداً، الخدمة بطيئة والطعام غير جيد.\",\n",
        "    \"الكتاب كان مقبولاً، بعض الفصول جيدة والبعض الآخر مملة.\"\n",
        "]\n",
        "\n",
        "for text in test_texts:\n",
        "    result = predict_sentiment(text)\n",
        "    print(f\"Text: {result['text']}\")\n",
        "    print(f\"Sentiment: {result['sentiment']} (Confidence: {result['confidence']:.4f})\")\n",
        "    print(\"-\" * 50)"
      ]
    }
