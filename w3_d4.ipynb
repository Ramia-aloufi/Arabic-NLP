      "source": [
        "from transformers import AutoModelForSequenceClassification, AutoTokenizer, Trainer, TrainingArguments\n",
        "import torch\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "from sklearn.metrics import accuracy_score, precision_recall_fscore_support\n",
        "from sklearn.model_selection import train_test_split\n",
        "from datasets import Dataset\n",
        "\n",
        "# Sample dataset creation function\n",
        "def create_sample_arabic_sentiment_dataset():\n",
        "    \"\"\"Create a small Arabic sentiment dataset for demonstration purposes\"\"\"\n",
        "    # This is just a toy example - in practice, you would use a proper dataset\n",
        "\n",
        "    positive_examples = [\n",
        "        \"أنا سعيد جدا بهذا المنتج، إنه رائع!\",\n",
        "        \"الخدمة ممتازة والموظفون ودودون\",\n",
        "        \"استمتعت كثيرا بالفيلم، أنصح الجميع بمشاهدته\",\n",
        "        \"المطعم نظيف والطعام لذيذ جدا\",\n",
        "        \"تجربة رائعة، سأعود مرة أخرى بالتأكيد\",\n",
        "        \"الكتاب مفيد ومثير للاهتمام، قرأته في يوم واحد\",\n",
        "        \"أحب هذا الهاتف، البطارية تدوم طويلا\",\n",
        "        \"شكرا لكم على الخدمة السريعة والمهنية\",\n",
        "        \"المدرس شرح الدرس بطريقة واضحة وممتعة\",\n",
        "        \"التطبيق سهل الاستخدام وسريع\"\n",
        "    ]\n",
        "\n",
        "    negative_examples = [\n",
        "        \"المنتج سيء جدا، لا أنصح به أبدا\",\n",
        "        \"الخدمة بطيئة والموظفون غير مهتمين\",\n",
        "        \"الفيلم مملّ وضيعت وقتي في مشاهدته\",\n",
        "        \"المطعم قذر والطعام سيء\",\n",
        "        \"تجربة سيئة، لن أعود مرة أخرى\",\n",
        "        \"الكتاب مضيعة للوقت والمال\",\n",
        "        \"لا أحب هذا الهاتف، البطارية ضعيفة\",\n",
        "        \"الخدمة كانت سيئة للغاية وبطيئة\",\n",
        "        \"لم أفهم شرح المدرس، كان مربكًا\",\n",
        "        \"التطبيق يتعطل كثيرًا ويستهلك البطارية\"\n",
        "    ]\n",
        "\n",
        "    neutral_examples = [\n",
        "        \"المنتج عادي، ليس سيئًا ولا ممتازًا\",\n",
        "        \"الخدمة كانت معقولة\",\n",
        "        \"الفيلم كان عاديًا، بعض المشاهد جيدة والبعض الآخر مملّ\",\n",
        "        \"الطعام في المطعم عادي\",\n",
        "        \"التجربة كانت متوسطة\",\n",
        "        \"الكتاب يحتوي على بعض المعلومات المفيدة\",\n",
        "        \"الهاتف يعمل بشكل طبيعي\",\n",
        "        \"الخدمة كانت كما هو متوقع\",\n",
        "        \"شرح المدرس كان مفهومًا في معظم الأحيان\",\n",
        "        \"التطبيق يعمل بشكل عادي\"\n",
        "    ]\n",
        "\n",
        "    # Combine examples and labels\n",
        "    texts = positive_examples + negative_examples + neutral_examples\n",
        "    labels = [2] * len(positive_examples) + [0] * len(negative_examples) + [1] * len(neutral_examples)\n",
        "\n",
        "    # Create DataFrame\n",
        "    df = pd.DataFrame({\n",
        "        'text': texts,\n",
        "        'label': labels\n",
        "    })\n",
        "\n",
        "    # Split into train and test sets\n",
        "    train_df, test_df = train_test_split(df, test_size=0.2, random_state=42, stratify=df['label'])\n",
        "\n",
        "    print(f\"Created dataset with {len(train_df)} training examples and {len(test_df)} test examples\")\n",
        "    print(f\"Label distribution in training set: {train_df['label'].value_counts().to_dict()}\")\n",
        "\n",
        "    # Convert to Hugging Face Dataset format\n",
        "    train_dataset = Dataset.from_pandas(train_df)\n",
        "    test_dataset = Dataset.from_pandas(test_df)\n",
        "\n",
        "    return train_dataset, test_dataset\n",
        "\n",
        "# Create datasets\n",
        "train_dataset, test_dataset = create_sample_arabic_sentiment_dataset()\n",
        "\n",
        "# Function to train a simple sentiment classifier\n",
        "def train_arabic_sentiment_classifier(train_dataset, test_dataset):\n",
        "    # Load pre-trained model and tokenizer\n",
        "    model_name = \"UBC-NLP/MARBERT\"  # Good for Arabic dialects too\n",
        "\n",
        "    tokenizer = AutoTokenizer.from_pretrained(model_name)\n",
        "    model = AutoModelForSequenceClassification.from_pretrained(\n",
        "        model_name,\n",
        "        num_labels=3  # 0=negative, 1=neutral, 2=positive\n",
        "    )\n",
        "\n",
        "    # Preprocess function\n",
        "    def preprocess_function(examples):\n",
        "        return tokenizer(examples[\"text\"], padding=\"max_length\", truncation=True, max_length=128)\n",
        "\n",
        "    # Tokenize datasets\n",
        "    tokenized_train = train_dataset.map(preprocess_function, batched=True)\n",
        "    tokenized_test = test_dataset.map(preprocess_function, batched=True)\n",
        "\n",
        "    # Define metrics computation\n",
        "    def compute_metrics(pred):\n",
        "        labels = pred.label_ids\n",
        "        preds = pred.predictions.argmax(-1)\n",
        "        precision, recall, f1, _ = precision_recall_fscore_support(labels, preds, average='weighted')\n",
        "        acc = accuracy_score(labels, preds)\n",
        "        return {\n",
        "            'accuracy': acc,\n",
        "            'f1': f1,\n",
        "            'precision': precision,\n",
        "            'recall': recall\n",
        "        }\n",
        "\n",
        "    # Set up training arguments\n",
        "    training_args = TrainingArguments(\n",
        "        output_dir=\"./arabic_sentiment_model\",\n",
        "        num_train_epochs=3,\n",
        "        per_device_train_batch_size=4,\n",
        "        per_device_eval_batch_size=4,\n",
        "        warmup_steps=500,\n",
        "        weight_decay=0.01,\n",
        "        logging_dir=\"./logs\",\n",
        "        logging_steps=10,\n",
        "        eval_strategy=\"epoch\",\n",
        "        save_strategy=\"epoch\",\n",
        "        load_best_model_at_end=True,\n",
        "    )\n",
        "\n",
        "    # Create Trainer\n",
        "    trainer = Trainer(\n",
        "        model=model,\n",
        "        args=training_args,\n",
        "        train_dataset=tokenized_train,\n",
        "        eval_dataset=tokenized_test,\n",
        "        compute_metrics=compute_metrics,\n",
        "    )\n",
        "\n",
        "    # Train the model\n",
        "    print(\"Training the Arabic sentiment classifier...\")\n",
        "    trainer.train()\n",
        "\n",
        "    # Evaluate\n",
        "    print(\"\\nEvaluating the model...\")\n",
        "    evaluation_results = trainer.evaluate()\n",
        "\n",
        "    # Print results\n",
        "    print(\"\\nEvaluation Results:\")\n",
        "    for key, value in evaluation_results.items():\n",
        "        print(f\"{key}: {value:.4f}\")\n",
        "\n",
        "    return model, tokenizer\n",
        "\n",
        "# Train the model\n",
        "model, tokenizer = train_arabic_sentiment_classifier(train_dataset, test_dataset)\n",
        "\n",
        "# Create a prediction function for interactive testing\n",
        "def predict_sentiment(text, model, tokenizer):\n",
        "    inputs = tokenizer(text, return_tensors=\"pt\", truncation=True, max_length=128)\n",
        "    # Move inputs to the same device as the model\n",
        "    if torch.cuda.is_available():\n",
        "        device = torch.device(\"cuda\")\n",
        "    else:\n",
        "        device = torch.device(\"cpu\")\n",
        "    inputs = inputs.to(device)\n",
        "    model = model.to(device)\n",
        "\n",
        "    with torch.no_grad():\n",
        "        outputs = model(**inputs)\n",
        "\n",
        "    logits = outputs.logits\n",
        "    predicted_class = torch.argmax(logits, dim=1).item()\n",
        "\n",
        "    # Get probabilities\n",
        "    probs = torch.nn.functional.softmax(logits, dim=1)\n",
        "\n",
        "    # Convert class to sentiment\n",
        "    sentiments = {0: \"Negative\", 1: \"Neutral\", 2: \"Positive\"}\n",
        "    sentiment = sentiments[predicted_class]\n",
        "\n",
        "    return {\n",
        "        \"sentiment\": sentiment,\n",
        "        \"confidence\": probs[0][predicted_class].item(),\n",
        "        \"probabilities\": {\n",
        "            \"negative\": probs[0][0].item(),\n",
        "            \"neutral\": probs[0][1].item(),\n",
        "            \"positive\": probs[0][2].item()\n",
        "        }\n",
        "    }\n",
        "\n",
        "# Test with some examples\n",
        "test_examples = [\n",
        "    \"المنتج ممتاز، أنا راضٍ جدًا عن الشراء\",\n",
        "    \"لم أكن سعيدًا بالخدمة، كانت بطيئة جدًا\",\n",
        "    \"التجربة كانت عادية، ربما أجرب مرة أخرى\"\n",
        "]\n",
        "\n",
        "for text in test_examples:\n",
        "    result = predict_sentiment(text, model, tokenizer)\n",
        "    print(f\"\\nText: {text}\")\n",
        "    print(f\"Sentiment: {result['sentiment']}\")\n",
        "    print(f\"Confidence: {result['confidence']:.2%}\")\n",
        "    print(\"Probabilities:\")\n",
        "    for sentiment, prob in result[\"probabilities\"].items():\n",
        "        print(f\"  - {sentiment}: {prob:.2%}\")\n"
      ]
    },
