      "source": [
        "# Code for comparing Arabic models in Colab\n",
        "models = [\n",
        "    \"aubmindlab/bert-base-arabertv2\",  # AraBERT v2\n",
        "    \"UBC-NLP/MARBERT\",                 # MARBERT (specialized for dialectal Arabic)\n",
        "    \"CAMeL-Lab/bert-base-arabic-camelbert-mix\",  # CamelBERT\n",
        "    \"google/muril-base-cased\"          # MuRIL (multilingual model supporting Arabic)\n",
        "]\n",
        "\n",
        "import torch\n",
        "from transformers import AutoTokenizer, AutoModel\n",
        "import pandas as pd\n",
        "from time import time\n",
        "\n",
        "results = []\n",
        "\n",
        "for model_name in models:\n",
        "    print(f\"\\nAnalyzing {model_name}...\")\n",
        "\n",
        "    # Load tokenizer and get vocab size\n",
        "    start_time = time()\n",
        "    tokenizer = AutoTokenizer.from_pretrained(model_name)\n",
        "    tokenizer_load_time = time() - start_time\n",
        "    vocab_size = len(tokenizer.get_vocab())\n",
        "\n",
        "    # Load model and get parameter count\n",
        "    start_time = time()\n",
        "    model = AutoModel.from_pretrained(model_name)\n",
        "    model_load_time = time() - start_time\n",
        "    param_count = model.num_parameters()\n",
        "\n",
        "    # Memory estimation (rough approximation)\n",
        "    mem_size = param_count * 4 / (1024 ** 2)  # Size in MB (assuming float32)\n",
        "\n",
        "    # Test inference speed\n",
        "    sample_text = \"هذا اختبار لسرعة النموذج في معالجة النصوص العربية.\"\n",
        "    inputs = tokenizer(sample_text, return_tensors=\"pt\")\n",
        "\n",
        "    # Warm-up run\n",
        "    _ = model(**inputs)\n",
        "    print(\"Warm-up run completed.\")\n",
        "\n",
        "    print(\"Tokens:\", tokenizer.convert_ids_to_tokens(inputs.input_ids[0]))\n",
        "\n",
        "\n",
        "    # Timed run\n",
        "    start_time = time()\n",
        "    with torch.no_grad():  # Disable gradient calculation for inference\n",
        "        _ = model(**inputs)\n",
        "    inference_time = time() - start_time\n",
        "\n",
        "    # Collect results\n",
        "    results.append({\n",
        "        \"Model\": model_name,\n",
        "        \"Vocabulary Size\": vocab_size,\n",
        "        \"Parameters (millions)\": param_count / 1_000_000,\n",
        "        \"Memory (MB)\": mem_size,\n",
        "        \"Load Time (s)\": model_load_time,\n",
        "        \"Inference Time (ms)\": inference_time * 1000\n",
        "    })\n",
        "\n",
        "    # Clear memory\n",
        "    del model, tokenizer\n",
        "    torch.cuda.empty_cache() if torch.cuda.is_available() else None\n",
        "\n",
        "# Display results as a table\n",
        "comparison_df = pd.DataFrame(results)\n",
        "comparison_df = comparison_df.round(2)\n",
        "comparison_df"
      ]
    },
