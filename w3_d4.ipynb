        "from transformers import AutoTokenizer, AutoModelForSequenceClassification, pipeline\n",
        "from sklearn.metrics import classification_report, confusion_matrix\n",
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "\n",
        "# Load MarBERT model, which is effective for Arabic dialects and classification\n",
        "# We need to specify num_labels based on our classification task (5 categories)\n",
        "model_name = \"UBC-NLP/MARBERT\"\n",
        "tokenizer = AutoTokenizer.from_pretrained(model_name)\n",
        "model = AutoModelForSequenceClassification.from_pretrained(model_name, num_labels=5)\n",
        "\n",
        "# Define label mapping for our classification task\n",
        "# Note: Since we're using a pre-trained model without fine-tuning, we need to map the output indices to our categories\n",
        "id2label = {\n",
        "    0: \"Politics\",\n",
        "    1: \"Sports\",\n",
        "    2: \"Business\",\n",
        "    3: \"Entertainment\",\n",
        "    4: \"Technology\"\n",
        "}\n",
        "model.config.id2label = id2label\n",
        "model.config.label2id = {v: k for k, v in id2label.items()}\n",
        "\n",
        "# Create classification pipeline\n",
        "classifier = pipeline(\"text-classification\", model=model, tokenizer=tokenizer)\n",
        "\n",
        "# Sample Arabic news headlines/snippets with their expected categories\n",
        "news_samples = [\n",
        "    {\"text\": \"الحكومة تعلن عن سلسلة إجراءات جديدة لدعم الاقتصاد المحلي\", \"category\": \"Business\"},\n",
        "    {\"text\": \"فوز المنتخب المصري على نظيره السعودي في مباراة حماسية\", \"category\": \"Sports\"},\n",
        "    {\"text\": \"إطلاق أحدث هاتف ذكي بمواصفات متطورة وسعر تنافسي\", \"category\": \"Technology\"},\n",
        "    {\"text\": \"الرئيس يلتقي بنظيره الفرنسي لبحث سبل التعاون المشترك\", \"category\": \"Politics\"},\n",
        "    {\"text\": \"مهرجان القاهرة السينمائي يعلن عن قائمة الأفلام المشاركة\", \"category\": \"Entertainment\"},\n",
        "    {\"text\": \"ارتفاع أسعار النفط عالمياً يؤثر على الأسواق المحلية\", \"category\": \"Business\"},\n",
        "    {\"text\": \"انطلاق بطولة كأس العالم لكرة القدم وسط إجراءات احترازية\", \"category\": \"Sports\"},\n",
        "    {\"text\": \"شركة مايكروسوفت تعلن عن إطلاق نظام تشغيل جديد\", \"category\": \"Technology\"},\n",
        "    {\"text\": \"توقيع اتفاقية سلام بين دولتين بعد سنوات من النزاع\", \"category\": \"Politics\"},\n",
        "    {\"text\": \"الممثل المصري يفوز بجائزة أفضل ممثل في مهرجان كان\", \"category\": \"Entertainment\"}\n",
        "]\n",
        "\n",
        "# Classify news samples\n",
        "results = []\n",
        "for sample in news_samples:\n",
        "    prediction = classifier(sample[\"text\"])[0]\n",
        "    results.append({\n",
        "        \"text\": sample[\"text\"],\n",
        "        \"true_category\": sample[\"category\"],\n",
        "        \"predicted_category\": prediction[\"label\"],\n",
        "        \"confidence\": prediction[\"score\"]\n",
        "    })\n",
        "\n",
        "# Convert to DataFrame\n",
        "df_results = pd.DataFrame(results)\n",
        "print(df_results)\n",
        "\n",
        "# Evaluate performance\n",
        "y_true = df_results[\"true_category\"].tolist()\n",
        "y_pred = df_results[\"predicted_category\"].tolist()\n",
        "\n",
        "# Print classification report\n",
        "print(\"\\nClassification Report:\")\n",
        "print(classification_report(y_true, y_pred))\n",
        "\n",
        "# Create confusion matrix\n",
        "categories = sorted(list(set(y_true + y_pred)))\n",
        "cm = confusion_matrix(y_true, y_pred, labels=categories)\n",
        "\n",
        "# Plot confusion matrix\n",
        "plt.figure(figsize=(10, 8))\n",
        "sns.heatmap(cm, annot=True, fmt='d', cmap='Blues', xticklabels=categories, yticklabels=categories)\n",
        "plt.title('Confusion Matrix - Arabic News Classification')\n",
        "plt.ylabel('True Category')\n",
        "plt.xlabel('Predicted Category')\n",
        "plt.tight_layout()\n",
        "plt.savefig('arabic_classification_cm.png')\n",
        "plt.show()\n",
        "\n",
        "# Analyze misclassifications\n",
        "misclassified = df_results[df_results[\"true_category\"] != df_results[\"predicted_category\"]]\n",
        "if len(misclassified) > 0:\n",
        "    print(\"\\nMisclassified samples:\")\n",
        "    for i, row in misclassified.iterrows():\n",
        "        print(f\"Text: {row['text']}\")\n",
        "        print(f\"True category: {row['true_category']}\")\n",
        "        print(f\"Predicted as: {row['predicted_category']} (confidence: {row['confidence']:.4f})\")\n",
        "        print(\"-\" * 50)\n",
        "else:\n",
        "    print(\"\\nAll samples were correctly classified!\")"
      ]
    },
