        "import re\n",
        "import pyarabic.araby as araby\n",
        "from google.colab import output\n",
        "# Enabling RTL display for better Arabic text visualization in Colab\n",
        "output.enable_custom_widget_manager()\n",
        "\n",
        "def preprocess_arabic_text(text):\n",
        "    \"\"\"\n",
        "    Preprocess Arabic text with common normalization steps\n",
        "    \"\"\"\n",
        "    # Original text\n",
        "    print(f\"Original: {text}\")\n",
        "\n",
        "    # Step 1: Remove diacritics (tashkeel)\n",
        "    text_no_tashkeel = araby.strip_tashkeel(text)\n",
        "    print(f\"Without diacritics: {text_no_tashkeel}\")\n",
        "\n",
        "    # Step 2: Remove tatweel (stretching character)\n",
        "    text_no_tatweel = araby.strip_tatweel(text_no_tashkeel)\n",
        "    print(f\"Without tatweel: {text_no_tatweel}\")\n",
        "\n",
        "    # Step 3: Normalize different forms of Hamza\n",
        "    text_norm_hamza = araby.normalize_hamza(text_no_tatweel)\n",
        "    print(f\"Normalized hamza: {text_norm_hamza}\")\n",
        "\n",
        "    # Step 4: Remove non-Arabic characters\n",
        "    text_arabic_only = re.sub(r'[^\\u0600-\\u06FF\\s]', ' ', text_norm_hamza)\n",
        "    print(f\"Arabic only: {text_arabic_only}\")\n",
        "\n",
        "    # Step 5: Remove extra whitespace\n",
        "    text_clean = re.sub(r'\\s+', ' ', text_arabic_only).strip()\n",
        "    print(f\"Final cleaned: {text_clean}\")\n",
        "\n",
        "    return text_clean\n",
        "\n",
        "# Example texts\n",
        "texts = [\n",
        "    \"السَّلَامُ عَلَيْكُمْ وَرَحْمَةُ اللهِ وَبَرَكَاتُهُ\",  # With tashkeel (diacritics)\n",
        "    \"مـــرحبــــا بكـــــم في دورة معـــالجة اللغـــة العـــربية\",  # With tatweel (stretching)\n",
        "    \"هذا النص يحتوي على أشكال مختلفة من الهمزة: أ، إ، آ، ء، ؤ، ئ\",  # Different hamza forms\n",
        "    \"Hello مرحباً world! تجربة123\"  # Mixed text\n",
        "]\n",
        "\n",
        "# Apply preprocessing to each example\n",
        "print(\"Arabic Text Normalization Examples:\")\n",
        "print(\"=\" * 50)\n",
        "for i, text in enumerate(texts):\n",
        "    print(f\"\\nExample {i+1}:\")\n",
        "    print(\"-\" * 30)\n",
        "    cleaned = preprocess_arabic_text(text)\n",
        "    print(\"-\" * 50)"
      ]
    },
